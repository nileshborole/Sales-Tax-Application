<?xml version="1.0" encoding="UTF-8"?>
<io-config name="test-io-config">

    <input-parser>
        <id>test</id>
        <criteria-list>
            <criteria>
                <key>testKey</key>
                <regex>^My\s+name\s+is\s+(?&#60;name&#62;[a-zA-z]*).\s+My\s+age\s+is\s+(?&#60;age&#62;[0-9]*).$</regex>
                <group-keys>name,age</group-keys>
            </criteria>
        </criteria-list>
    </input-parser>

</io-config>